<h1>New tweet</h1>

<% form_for(@tweet) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :tweet_id %><br />
    <%= f.text_field :tweet_id %>
  </p>
  <p>
    <%= f.label :twitterer_id %><br />
    <%= f.text_field :twitterer_id %>
  </p>
  <p>
    <%= f.label :text %><br />
    <%= f.text_field :text %>
  </p>
  <p>
    <%= f.label :source %><br />
    <%= f.text_field :source %>
  </p>
  <p>
    <%= f.label :truncated %><br />
    <%= f.check_box :truncated %>
  </p>
  <p>
    <%= f.label :in_reply_to_status_id %><br />
    <%= f.text_field :in_reply_to_status_id %>
  </p>
  <p>
    <%= f.label :in_reply_to_user_id %><br />
    <%= f.text_field :in_reply_to_user_id %>
  </p>
  <p>
    <%= f.label :favorited %><br />
    <%= f.check_box :favorited %>
  </p>
  <p>
    <%= f.label :in_reply_to_screen_name %><br />
    <%= f.text_field :in_reply_to_screen_name %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', tweets_path %>